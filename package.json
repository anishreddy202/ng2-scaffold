{
  "name": "ng2-scaffold",
  "version": "0.0.1",
  "description": "Angular2 scaffold with webpack and express",
  "author": {
    "name": "Irving UI",
    "email": "irvui@verizon.com"
  },
  "contributors": [
    {
      "name": "Anish Madhi",
      "email": "anish.m.reddy@verizon.com"
    }
  ],
  "engines": {
    "node": "6.7.0",
    "npm": "3.10.8"
  },
  "scripts": {
    "start": "node server/index.js & npm run serve",
    "serve": "webpack-dev-server --inline --progress",
    "build": "rimraf dist && NODE_ENV='production' webpack --config config/webpack.prod.js -p --progress --bail && npm run copy",
    "copy": "webpack --config config/webpack.copy.js",
    "test": "npm run test:client && npm run test:server",
    "test:client": "karma start karma.conf.js",
    "test:server": "NODE_ENV='test' mocha server --recursive",
    "test:e2e": "npm run kill && (npm run serve:dev & protractor protractor.config.js)",
    "webdriver:update": "webdriver-manager update",
    "kill": "kill -9 $(lsof -i tcp:8080)",
    "docker": "npm run build && docker build -t irvui/ng2-scaffold:latest .",
    "docker:start": "docker run -p 3000:3000 irvui/ng2-scaffold"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^2.1.1",
    "@angular/compiler": "^2.1.1",
    "@angular/core": "^2.1.1",
    "@angular/forms": "^2.1.1",
    "@angular/http": "^2.1.1",
    "@angular/platform-browser": "^2.1.1",
    "@angular/platform-browser-dynamic": "^2.1.1",
    "@angular/router": "^3.1.1",
    "bunyan": "^1.8.3",
    "bunyan-format": "^0.2.1",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "lodash": "^4.16.4",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/node": "^6.0.45",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.4",
    "chai": "^4.0.0-canary.1",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.0",
    "jasmine": "^2.5.2",
    "jasmine-spec-reporter": "^2.7.0",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "karma-webpack": "^1.8.0",
    "node-sass": "^3.10.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-helpers": "^1.1.2",
    "ts-loader": "^0.9.5",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
